<view class="logAudit" bind:__l="__l"><van-panel vue-slots="{{['default','footer']}}"><view><van-cell title="生产日志编号" value="{{disData.logCode}}" size="large" data-com-type="wx"></van-cell><van-cell title="实际生产数量" value="{{disData.actualProduceQuantity}}" size="large" data-com-type="wx"></van-cell><van-cell title="实际生产时间" value="{{disData.actualCompleteTime}}" size="large" data-com-type="wx"></van-cell><van-cell title="填报时间" value="{{disData.createTime}}" size="large" data-com-type="wx"></van-cell><van-cell title="款式编号" value="{{disData.styleCode}}" size="large" data-com-type="wx"></van-cell><van-cell title="商家编号" value="{{disData.itemCode}}" size="large" data-com-type="wx"></van-cell><van-cell title="货品编号" value="{{disData.itemCode}}" size="large" data-com-type="wx"></van-cell><van-cell title="状态" value="{{disData.auditStatus=='sh02'?'审核中':disData.auditStatus=='sh01'?'审核通过':'审核驳回'}}" size="large" data-com-type="wx"></van-cell><van-cell title="商品名称" value="{{disData.goodsName}}" size="large" data-com-type="wx"></van-cell><van-cell title="填报人" value="{{disData.createName}}" size="large" data-com-type="wx"></van-cell></view><view slot="footer"></view></van-panel><prompt visible="{{promptVisible}}" title="拒绝原因" data-event-opts="{{[['^updateVisible',[['__set_sync',['$0','promptVisible','$event'],['']]]],['^confirm',[['clickPromptConfirm']]],['^confirms',[['cancel']]]]}}" bind:updateVisible="__e" bind:confirm="__e" bind:confirms="__e"></prompt><van-tabbar active-color="#7d7e80" data-com-type="wx" vue-slots="{{['default']}}"><van-tabbar-item icon="close" data-event-opts="{{[['^click',[['Refused']]]]}}" data-com-type="wx" bind:click="__e" vue-slots="{{['default']}}">审核驳回</van-tabbar-item><van-tabbar-item icon="passed" data-event-opts="{{[['^click',[['accept']]]]}}" data-com-type="wx" bind:click="__e" vue-slots="{{['default']}}">审核通过</van-tabbar-item></van-tabbar></view>