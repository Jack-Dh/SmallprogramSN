<view class="logAudit" bind:__l="__l"><van-panel vue-slots="{{['default','footer']}}"><view><van-cell title="生产日志编号" value="{{disData.logCode}}" size="large" data-com-type="wx"></van-cell><van-cell title="实际生产数量（盒）" value="{{disData.actualProduceQuantity}}" size="large" data-com-type="wx"></van-cell><van-cell title="实际生产时间" value="{{TIME}}" size="large" data-com-type="wx"></van-cell><van-cell title="填报时间" value="{{disData.createTime}}" size="large" data-com-type="wx"></van-cell><van-cell title="派单编号" value="{{disData.dispatchCode}}" size="large" data-com-type="wx"></van-cell><van-cell title="状态" value="{{disData.auditStatus=='sh02'?'审核中':disData.auditStatus=='sh01'?'审核通过':'审核驳回'}}" size="large" data-com-type="wx"></van-cell><van-cell title="填报人" value="{{disData.applicant}}" size="large" data-com-type="wx"></van-cell><van-collapse border="{{false}}" value="{{activeNames}}" accordion data-event-opts="{{[['^change',[['onChange']]]]}}" data-com-type="wx" bind:change="__e" vue-slots="{{['default']}}"><van-collapse-item title="商品详情" name="1" data-com-type="wx" vue-slots="{{['default']}}"><block wx:for="{{disData.goodsBeanList}}" wx:for-item="item" wx:for-index="__i0__"><view class="goodsdetails"><view><view>{{"商品名称:"+item.name}}</view><view>{{"商家编码:"+item.merchantCode}}</view><view>{{"货品编号:"+item.itemCode}}</view><view>{{"颜色:"+item.colour}}</view><view>{{"品牌:"+item.brand}}</view><view>{{"包装材料:"+item.packag}}</view><view>{{"克重:"+item.weight}}</view><view>{{"面料成分:"+item.ingredients}}</view><view>{{"规格（双）:"+item.specifications}}</view><view>{{"生产数量（双）:"+item.dispatchQuantity}}</view></view></view></block></van-collapse-item></van-collapse></view><view slot="footer"></view></van-panel><prompt visible="{{promptVisible}}" title="拒绝原因" data-event-opts="{{[['^updateVisible',[['__set_sync',['$0','promptVisible','$event'],['']]]],['^confirm',[['clickPromptConfirm']]],['^confirms',[['cancel']]]]}}" bind:updateVisible="__e" bind:confirm="__e" bind:confirms="__e"></prompt><view style="height:150rpx;"></view><van-tabbar active-color="#7d7e80" data-com-type="wx" vue-slots="{{['default']}}"><van-tabbar-item icon="close" data-event-opts="{{[['^click',[['Refused']]]]}}" data-com-type="wx" bind:click="__e" vue-slots="{{['default']}}">审核驳回</van-tabbar-item><van-tabbar-item icon="passed" data-event-opts="{{[['^click',[['accept']]]]}}" data-com-type="wx" bind:click="__e" vue-slots="{{['default']}}">审核通过</van-tabbar-item></van-tabbar></view>