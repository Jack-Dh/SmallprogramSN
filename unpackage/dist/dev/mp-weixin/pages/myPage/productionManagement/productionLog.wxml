<view class="productionLog" bind:__l="__l"><van-panel title="生产日志" data-com-type="wx" vue-slots="{{['default']}}"><view><van-cell title="派单编号" value="{{disData.dispatchCode}}" size="large" data-com-type="wx"></van-cell><van-cell title="派工工艺" value="{{disData.processNode=='weave'?'织造':disData.processNode=='seamHead'?'缝头':disData.processNode=='stereoType'?'定型':'包装'}}" size="large" data-com-type="wx"></van-cell><van-cell title="预加工时间" value="{{disData.expectProcessTime}}" size="large" data-com-type="wx"></van-cell><van-cell title="预完工时间" value="{{disData.expectCompleteTime}}" size="large" data-com-type="wx"></van-cell></view></van-panel><view class="timebox"><van-field label="预计生产数量" type="number" value="{{expectProduceQuantity}}" data-event-opts="{{[['^change',[['expectOnChange']]]]}}" data-com-type="wx" bind:change="__e"></van-field><van-field label="实际生产数量" type="number" value="{{actualProduceQuantity}}" data-event-opts="{{[['^change',[['actualOnChange']]]]}}" data-com-type="wx" bind:change="__e"></van-field></view><view class="timebox"><van-cell-group title="实际生产时间" data-com-type="wx" vue-slots="{{['default']}}"><rui-date-picker fields="second" start="2010-00-00 00:00:00" end="2030-12-30 23:59:59" value="{{actualProduceTime}}" data-event-opts="{{[['^change',[['saveTime']]]]}}" bind:change="__e"></rui-date-picker></van-cell-group></view><view class="btnstyle"><van-button type="primary" size="normal" data-event-opts="{{[['^click',[['saveProLog']]]]}}" data-com-type="wx" bind:click="__e" vue-slots="{{['default']}}">保存</van-button></view></view>