<view class="productionLog" bind:__l="__l"><van-panel vue-slots="{{['default']}}"><view><van-cell title="派单编号" value="{{disData.dispatchCode}}" size="large" data-com-type="wx"></van-cell><van-cell title="款式编号" value="{{item.styleCode}}" size="large" data-com-type="wx"></van-cell><van-cell title="派工工艺" value="{{disData.processNode=='weave'?'织造':disData.processNode=='seamHead'?'缝头':disData.processNode=='stereoType'?'定型':'包装'}}" size="large" data-com-type="wx"></van-cell><van-cell title="预加工时间" value="{{disData.expectProcessTime}}" size="large" data-com-type="wx"></van-cell><van-cell title="预完工时间" value="{{disData.expectCompleteTime}}" size="large" data-com-type="wx"></van-cell><van-cell title="剩余生产总量" value="{{disData.restNumber}}" size="large" data-com-type="wx"></van-cell></view></van-panel><view class="timebox"><van-field label="实际生产数量" required error-message="{{errorMessage}}" type="number" value="{{actualProduceQuantity}}" data-event-opts="{{[['^change',[['actualOnChange']]]]}}" data-com-type="wx" bind:change="__e"></van-field></view><view data-event-opts="{{[['tap',[['e0',['$event']]]]]}}" class="timebox" bindtap="__e"><van-field label="实际生产时间" readonly value="{{actualProduceTime}}" data-com-type="wx"></van-field></view><view class="btnstyle"><w-picker class="vue-ref" mode="{{mode}}" startYear="2019" endYear="2030" step="1" defaultVal="{{defaultVal}}" themeColor="#f00" data-ref="picker" data-event-opts="{{[['^confirm',[['onConfirm']]]]}}" bind:confirm="__e"></w-picker><van-button type="info" size="normal" data-event-opts="{{[['^click',[['saveProLog']]]]}}" data-com-type="wx" bind:click="__e" vue-slots="{{['default']}}">提交</van-button><van-button type="primary" size="normal" data-event-opts="{{[['^click',[['history']]]]}}" data-com-type="wx" bind:click="__e" vue-slots="{{['default']}}">历史提交</van-button></view></view>