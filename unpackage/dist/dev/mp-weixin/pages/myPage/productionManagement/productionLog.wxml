<view class="productionLog" bind:__l="__l"><van-panel vue-slots="{{['default']}}"><view><van-cell title="派单编号" value="{{disData.dispatchCode}}" size="large" data-com-type="wx"></van-cell><van-cell title="款式编号" value="{{disData.styleCode}}" size="large" data-com-type="wx"></van-cell><van-cell title="派工工艺" value="{{disData.processNode=='weave'?'织造':disData.processNode=='seamHead'?'缝头':disData.processNode=='stereoType'?'定型':'包装'}}" size="large" data-com-type="wx"></van-cell><van-cell title="完结状态" value="{{disData.receiveState==1?'未完结':'已完结'}}" size="large" data-com-type="wx"></van-cell><van-cell title="预加工时间" value="{{disData.expectProcessTime}}" size="large" data-com-type="wx"></van-cell><van-cell title="预完工时间" value="{{disData.expectCompleteTime}}" size="large" data-com-type="wx"></van-cell><van-cell title="生产总量(盒)" value="{{disData.totalNumber}}" size="large" data-com-type="wx"></van-cell><van-cell title="剩余生产总量(盒)" value="{{disData.restNumber}}" size="large" data-com-type="wx"></van-cell><van-cell title="选择商品" is-link data-event-opts="{{[['^click',[['e0']]]]}}" data-com-type="wx" bind:click="__e"></van-cell></view></van-panel><van-popup show="{{popShow}}" position="top" close-on-click-overlay data-event-opts="{{[['^close',[['onClose']]],['^clickOverlay',[['e1']]]]}}" data-com-type="wx" bind:close="__e" bind:clickOverlay="__e" vue-slots="{{['default']}}"><van-checkbox-group value="{{resultList}}" data-event-opts="{{[['^change',[['onChangeSelect']]]]}}" data-com-type="wx" bind:change="__e" vue-slots="{{['default']}}"><van-cell-group vue-slots="{{['default']}}"><block wx:for="{{disData.goodsList}}" wx:for-item="item" wx:for-index="index" wx:key="index"><van-cell clickable data-name="{{item.name}}" data-event-opts="{{[['^click',[['toggle']]]]}}" data-com-type="wx" bind:click="__e" vue-slots="{{['default','title']}}"><view slot="title"><view>{{item.name}}</view><view style="color:#999999;"><view>{{"SKU:"+item.sku}}</view><view>{{"规格(双):"+item.specifications}}</view></view></view><van-checkbox name="{{item.goodsUuid}}" data-com-type="wx"></van-checkbox></van-cell></block></van-cell-group></van-checkbox-group></van-popup><view class="timebox"><van-field label="实际生产数量" required error-message="{{errorMessage}}" type="number" value="{{actualProduceQuantity}}" data-event-opts="{{[['^change',[['actualOnChange']]]]}}" data-com-type="wx" bind:change="__e"></van-field></view><view data-event-opts="{{[['tap',[['e2',['$event']]]]]}}" class="timebox" bindtap="__e"><van-field label="实际生产时间" readonly required error-message="{{errorMessageTime}}" value="{{actualProduceTime}}" data-com-type="wx"></van-field></view><view class="btnstyle"><w-picker class="vue-ref" mode="{{mode}}" endYear="{{endYear}}" defaultVal="{{defaultVals}}" themeColor="#f00" data-ref="picker" data-event-opts="{{[['^confirm',[['onConfirm']]]]}}" bind:confirm="__e"></w-picker><van-button type="info" disabled="{{numberState}}" size="normal" data-event-opts="{{[['^click',[['saveProLog']]]]}}" data-com-type="wx" bind:click="__e" vue-slots="{{['default']}}">提交</van-button><van-button type="primary" size="normal" data-event-opts="{{[['^click',[['history']]]]}}" data-com-type="wx" bind:click="__e" vue-slots="{{['default']}}">历史提交</van-button></view></view>