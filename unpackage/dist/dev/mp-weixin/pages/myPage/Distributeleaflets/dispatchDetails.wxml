<view class="dispatchDetails" bind:__l="__l"><van-panel title="派单详情" data-com-type="wx" vue-slots="{{['default','footer']}}"><view><van-cell title="派单编号" value="{{disData.dispatchCode}}" size="large" data-com-type="wx"></van-cell><van-cell title="派单工厂" value="{{disData.factoryName}}" size="large" data-com-type="wx"></van-cell><van-cell title="预加工时间" value="{{disData.expectProcessTime}}" size="large" data-com-type="wx"></van-cell><van-cell title="预完工时间" value="{{disData.expectCompleteTime}}" size="large" data-com-type="wx"></van-cell><van-cell title="款式编号" value="{{disData.styleCode}}" size="large" data-com-type="wx"></van-cell><van-cell title="派工工艺" value="{{disData.processNode=='weave'?'织造':disData.processNode=='seamHead'?'缝头':disData.processNode=='stereoType'?'定型':'包装'}}" size="large" data-com-type="wx"></van-cell><van-cell title="加工数量" value="{{disData.totalNumber}}" size="large" data-com-type="wx"></van-cell><van-cell title="接受状态" value="{{disData.receiveState==0?'待处理':disData.receiveState==1?'已接单':'已拒绝'}}" size="large" data-com-type="wx"></van-cell><van-cell title="派工人员" value="{{disData.dispatchWorker}}" size="large" data-com-type="wx"></van-cell><van-cell title="派工时间" value="{{disData.createTime}}" size="large" data-com-type="wx"></van-cell></view><view slot="footer"></view></van-panel><view class="boxButon"><van-button type="danger" disabled="{{butState}}" data-event-opts="{{[['^click',[['Refused']]]]}}" data-com-type="wx" bind:click="__e" vue-slots="{{['default']}}">拒绝</van-button><van-button type="primary" disabled="{{butState}}" data-event-opts="{{[['^click',[['accept']]]]}}" data-com-type="wx" bind:click="__e" vue-slots="{{['default']}}">接受</van-button></view><prompt visible="{{promptVisible}}" title="拒绝原因" data-event-opts="{{[['^updateVisible',[['__set_sync',['$0','promptVisible','$event'],['']]]],['^confirm',[['clickPromptConfirm']]],['^confirms',[['cancel']]]]}}" bind:updateVisible="__e" bind:confirm="__e" bind:confirms="__e"></prompt></view>