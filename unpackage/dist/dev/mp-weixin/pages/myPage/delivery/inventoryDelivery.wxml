<view class="productionLog" bind:__l="__l"><van-panel vue-slots="{{['default']}}"><view><van-cell title="派单编号" value="{{disData.dispatchCode}}" size="large" data-com-type="wx"></van-cell><van-cell title="款式编号" value="{{disData.styleCode}}" size="large" data-com-type="wx"></van-cell><van-cell title="派工工艺" value="{{disData.processNode=='weave'?'织造':disData.processNode=='seamHead'?'缝头':disData.processNode=='stereoType'?'定型':'包装'}}" size="large" data-com-type="wx"></van-cell><van-cell title="完结状态" value="{{disData.receiveState==1?'未完结':'已完结'}}" size="large" data-com-type="wx"></van-cell><van-cell title="库存总量" value="{{disData.totalSendGoodsQuantity}}" size="large" data-com-type="wx"></van-cell><van-cell title="选择商品" is-link data-event-opts="{{[['^click',[['e0']]]]}}" data-com-type="wx" bind:click="__e"></van-cell></view></van-panel><van-popup show="{{popShow}}" position="top" close-on-click-overlay="{{true}}" data-event-opts="{{[['^clickOverlay',[['e1']]]]}}" data-com-type="wx" bind:clickOverlay="__e" vue-slots="{{['default']}}"><block wx:for="{{disData.goodsList}}" wx:for-item="item" wx:for-index="index"><van-card title="{{item.name}}" data-com-type="wx" vue-slots="{{['desc']}}"><view slot="desc"><view>{{"颜色："+item.color}}</view><view>{{"sku："+item.sku}}</view><view>{{"库存数量："+item.inventoryQuantity}}</view><van-tag type="danger" size="medium" data-com-type="wx" vue-slots="{{['default']}}">{{item.brand}}</van-tag><view style="display:flex;justify-content:space-between;align-items:center;"><van-stepper value="{{item.actualSendGoodsQuantity}}" min="0" max="{{item.inventoryQuantity}}" data-event-opts="{{[['^change',[['changeStepper']]],['^blur',[['blurStepper',[index]]]],['^plus',[['blurStepper',[index]]]],['^minus',[['blurStepper',[index]]]]]}}" data-com-type="wx" bind:change="__e" bind:blur="__e" bind:plus="__e" bind:minus="__e"></van-stepper><text>{{"X"+item.actualSendGoodsQuantity}}</text></view></view></van-card></block><van-goods-action vue-slots="{{['default']}}"><van-goods-action-icon icon="cart-o" text="数量" info="{{totalNumber}}" data-com-type="wx"></van-goods-action-icon><van-goods-action-icon icon="arrow-up" text="返回" data-event-opts="{{[['^click',[['e2']]]]}}" data-com-type="wx" bind:click="__e"></van-goods-action-icon><van-goods-action-button text="发货" data-event-opts="{{[['^click',[['delivery']]]]}}" data-com-type="wx" bind:click="__e"></van-goods-action-button></van-goods-action></van-popup></view>