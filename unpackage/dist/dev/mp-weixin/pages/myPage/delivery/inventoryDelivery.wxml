<view class="productionLog" bind:__l="__l"><van-panel vue-slots="{{['default']}}"><view><van-cell title="派单编号" value="{{disData.dispatchCode}}" size="large" data-com-type="wx"></van-cell><van-cell title="款式编号" value="{{disData.styleCode}}" size="large" data-com-type="wx"></van-cell><van-cell title="派工工艺" value="{{disData.processNode=='weave'?'织造':disData.processNode=='seamHead'?'缝头':disData.processNode=='stereoType'?'定型':'包装'}}" size="large" data-com-type="wx"></van-cell><van-cell title="完结状态" value="{{disData.receiveState==1?'未完结':'已完结'}}" size="large" data-com-type="wx"></van-cell><van-cell title="剩余发货数量（盒）" value="{{disData.totalSendGoodsQuantity}}" size="large" data-com-type="wx"></van-cell><van-cell required title="选择商品" is-link data-event-opts="{{[['^click',[['e0']]]]}}" data-com-type="wx" bind:click="__e"></van-cell></view><view class="timebox"><van-field label="实际发货数量" required error-message="{{errorMessage}}" type="number" value="{{actualSendGoodsQuantity}}" data-event-opts="{{[['^change',[['actualOnChange']]]]}}" data-com-type="wx" bind:change="__e"></van-field></view></van-panel><van-popup show="{{popShow}}" position="top" close-on-click-overlay="{{true}}" data-event-opts="{{[['^clickOverlay',[['e1']]]]}}" data-com-type="wx" bind:clickOverlay="__e" vue-slots="{{['default']}}"><van-checkbox-group value="{{resultList}}" data-event-opts="{{[['^change',[['onChangeSelect']]]]}}" data-com-type="wx" bind:change="__e" vue-slots="{{['default']}}"><van-cell-group vue-slots="{{['default']}}"><block wx:for="{{disData.goodsList}}" wx:for-item="item" wx:for-index="index" wx:key="index"><van-cell data-name="{{item.name}}" data-com-type="wx" vue-slots="{{['default','title']}}"><view slot="title"><view>{{item.name}}</view><view style="color:#999999;"><view>{{"SKU:"+item.sku}}</view><view>{{"库存数量(双):"+item.dispatchQuantity}}</view><view>{{"规格(双):"+item.specifications}}</view></view></view><van-checkbox name="{{item.goodsUuid}}" data-com-type="wx"></van-checkbox></van-cell></block></van-cell-group></van-checkbox-group></van-popup><view class="btnstyle"><van-button type="info" disabled="{{numberState}}" size="normal" data-event-opts="{{[['^click',[['delivery']]]]}}" data-com-type="wx" bind:click="__e" vue-slots="{{['default']}}">确认发货</van-button></view></view>