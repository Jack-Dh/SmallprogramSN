<view class="deliveDetails.vue" bind:__l="__l"><van-panel vue-slots="{{['default']}}"><view><van-cell title="发货编号" value="{{historydetailsList.sendCode}}" size="large" data-com-type="wx"></van-cell><van-cell title="派工编号" value="{{historydetailsList.dispatchCode}}" size="large" data-com-type="wx"></van-cell><van-cell title="发货总数" value="{{historydetailsList.sendsGoodsTotalQuantity}}" size="large" data-com-type="wx"></van-cell><van-cell title="状态" value="{{historydetailsList.auditStatus=='sh02'?'审核中':disData.auditStatus=='sh01'?'审核通过':'审核驳回'}}" size="large" data-com-type="wx"></van-cell><van-cell title="发货时间" value="{{historydetailsList.createTime}}" size="large" data-com-type="wx"></van-cell><van-collapse border="{{false}}" value="{{activeNames}}" accordion data-event-opts="{{[['^change',[['onChange']]]]}}" data-com-type="wx" bind:change="__e" vue-slots="{{['default']}}"><van-collapse-item title="商品详情" name="1" data-com-type="wx" vue-slots="{{['default']}}"><block wx:for="{{historydetailsList.goodsList}}" wx:for-item="item" wx:for-index="__i0__"><view class="goodsdetails"><view>{{"商品名称:"+item.name}}</view><view>{{"商家编码:"+item.merchantCode}}</view><view>{{"货品编号:"+item.itemCode}}</view><view>{{"颜色:"+item.colour}}</view><view>{{"品牌:"+item.brand}}</view><view>{{"包装材料:"+item.packag}}</view><view>{{"克重:"+item.weight}}</view><view>{{"面料成分:"+item.ingredients}}</view><view>{{"发货数量:"+item.actualSendGoodsQuantity}}</view></view></block></van-collapse-item></van-collapse></view></van-panel></view>