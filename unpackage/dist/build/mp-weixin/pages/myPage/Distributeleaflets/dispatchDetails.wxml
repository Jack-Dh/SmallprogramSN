<view class="dispatchDetails" bind:__l="__l"><van-panel title="派单详情" data-com-type="wx" vue-slots="{{['default','footer']}}"><view><van-cell title="派单编号" value="{{disData.dispatchCode}}" size="large" data-com-type="wx"></van-cell><van-cell title="派单工厂" value="{{disData.factoryName}}" size="large" data-com-type="wx"></van-cell><van-cell title="预加工时间" value="{{disData.expectProcessTime}}" size="large" data-com-type="wx"></van-cell><van-cell title="预完工时间" value="{{disData.expectCompleteTime}}" size="large" data-com-type="wx"></van-cell><van-cell title="派工工艺" value="{{disData.processNode=='weave'?'织造':disData.processNode=='seamHead'?'缝头':disData.processNode=='stereoType'?'定型':'包装'}}" size="large" data-com-type="wx"></van-cell><van-cell title="创建时间" value="{{disData.createTime}}" size="large" data-com-type="wx"></van-cell></view><view slot="footer"></view></van-panel></view>